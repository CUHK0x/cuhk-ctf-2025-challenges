+static JSValue js_array_oob(JSContext *ctx, JSValueConst this_val,
+                           int argc, JSValueConst *argv)
+{
+    JSValue obj, ret, *arrp;
+    int64_t idx;
+    uint32_t count;
+
+    obj = JS_ToObject(ctx, this_val);
+
+    if (argc < 1 || argc > 2) goto exception;
+    if(!js_get_fast_array(ctx, obj, &arrp, &count))
+        goto exception;
+    if (JS_ToInt64Sat(ctx, &idx, argv[0]))
+        goto exception;
+
+    if (argc == 1) {
+		ret = JS_DupValue(ctx, arrp[idx]);
+    } else {
+		set_value(ctx, &arrp[idx], JS_DupValue(ctx, argv[1]));
+        ret = JS_UNDEFINED;
+    }
+    JS_FreeValue(ctx, obj);
+	return ret;
+
+exception:
+    JS_FreeValue(ctx, obj);
+    return JS_EXCEPTION;
+}
+
 #define special_every    0
 #define special_some     1
 #define special_forEach  2
